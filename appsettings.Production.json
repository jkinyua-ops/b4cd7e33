{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "ConnectionStrings": {
    "DefaultSqlConnection": "Server=${DB_SERVER};Database=Nostra_Dataload;User Id=${DB_USER};Password=${DB_PASSWORD};TrustServerCertificate=True;MultipleActiveResultSets=true;Min Pool Size=5;Max Pool Size=100;Connection Timeout=30;",
    "TaskQueueConnection": "Server=${DB_SERVER};Database=Nostra_Dataload_Tasks;User Id=${DB_USER};Password=${DB_PASSWORD};TrustServerCertificate=True;MultipleActiveResultSets=true;Min Pool Size=5;Max Pool Size=100;Connection Timeout=30;"
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "https://app.nostra-app.com",
      "https://api.nostra-app.com"
    ]
  }
}